    "cookie-parser": "1.4.4",
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "express-handlebars": "3.1.0",
    "express-minify-html": "0.12.0",
    "express-session": "1.16.2",
    "firebase-admin": "8.1.0",
    "helmet": "3.18.0",
    "image-downloader": "3.4.2",
    "lodash": "4.17.11",
    "mongoose": "5.6.0",
    "morgan": "1.9.1",
    "node-fetch": "2.6.7",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
