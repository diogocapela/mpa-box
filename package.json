{
  "name": "mpa-box",
  "version": "0.0.1",
  "description": "CRUD multi-page application built using Node.js, Express and Handlebars.",
  "scripts": {
    "clean": "rm -rf node_modules",
    "test": "mocha \"test/**/*.test.js\" --exit",
    "start": "NODE_ENV=production node src/server.js",
    "nodemon": "NODE_ENV=development nodemon src/server.js",
    "webpack:watch": "webpack --mode development --watch",
    "webpack:build": "webpack --mode production",
    "dev": "npm-run-all --parallel webpack:watch nodemon",
    "build": "webpack --mode production",
    "build:sitemap": "node src/config/buildSitemap.js"
  },
  "dependencies": {
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
