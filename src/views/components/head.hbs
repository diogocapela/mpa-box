<html lang="en">

<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />

    <meta name="theme-color" content="#123751" />

    <!-- Manifest -->
    <link rel="manifest" href="/manifest.json" />

    <!-- Favicon -->
    <link rel="icon" href="/img/favicon.ico" />

    <!-- Title -->
    <title>{{meta.pageTitle}}</title>
    <meta property="og:title" content="{{meta.pageTitle}}" />
    <meta name="twitter:title" content="{{meta.pageTitle}}" />
    <meta itemprop="name" content="{{meta.pageTitle}}" />

    <!-- Description -->
    <meta name="description" content="{{meta.pageDescription}}" />
    <meta name="summary" content="{{meta.pageDescription}}" />
    <meta property="og:description" content="{{meta.pageDescription}}" />
    <meta name="twitter:description" content="{{meta.pageDescription}}" />
    <meta itemprop="description" content="{{meta.pageDescription}}" />
    <meta name="subject" content="{{meta.pageDescription}}" />
    <meta name="topic" content="{{meta.pageDescription}}" />
    <meta name="abstract" content="{{meta.pageDescription}}" />

    <!-- Image Thumbnail -->
    <meta property="og:image" content="{{meta.pageImageUrl}}" />
    <meta name="twitter:image:src" content="{{meta.pageImageUrl}}" />
    <meta itemprop="image" content="{{meta.pageImageUrl}}" />

    <!-- URL -->
    <meta name="url" content="{{meta.pageUrl}}" />
    <meta property="og:url" content="{{meta.pageUrl}}" />
    <link rel="canonical" href="{{meta.pageUrl}}" />
    <link rel="shortlink" href="{{meta.pageUrl}}" />

    <!-- Website Name -->
    <meta name="author" content="{{meta.websiteName}}" />
    <meta name="copyright" content="{{meta.websiteName}}" />
    <meta name="designer" content="{{meta.websiteName}}" />
    <meta name="owner" content="{{meta.websiteName}}" />
    <meta property="og:site_name" content="{{meta.websiteName}}" />

    <!-- Language & Location -->
    <meta name="language" content="en" />
    <meta property="og:country-name" content="Portugal" />

    <!-- Email -->
    <meta name="reply-to" content="{{meta.websiteEmail}}" />
    <meta property="og:email" content="{{meta.websiteEmail}}" />

    <!-- Random -->
    <meta name="keywords" content="mpa, box" />
    <meta property="og:type" content="website" />
    <meta name="twitter:site" content="{{meta.websiteTwitter}}" />
    <meta name="twitter:creator" content="{{meta.websiteTwitter}}" />
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="robots" content="index, follow" />

    <meta property="og:phone_number" content="+351-912-345-678" />
    <meta property="og:locality" content="Porto" />
    <meta property="og:country-name" content="Portugal" />

